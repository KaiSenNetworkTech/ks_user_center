<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunos.tv.appupgrade.dal.dao.TestGroupDAO">
	
	<select id="selectById" resultType="TestGroupDo" parameterType="long">
		select `id`,`name`
		FROM `test_group`
		WHERE
			`id` = #{id} AND `is_delete` = 0
		LIMIT 1
	</select>

</mapper>
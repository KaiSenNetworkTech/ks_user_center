<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunos.tv.appupgrade.dal.dao.DeviceModelDAO">

	<select id="selectByModelName" resultType="DeviceModelDo" parameterType="String">
		select `id`,`brand_id`,`brand_name`,`model_name`,`terminal_type`
		FROM `device_model`
		WHERE model_name = #{modelName}
		LIMIT 1
	</select>
</mapper>
<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunos.tv.appupgrade.dal.dao.ResourceDAO">

	<select id="selectById" resultType="ResourceDo" parameterType="long">
		select `id`,`name`,`code`,`description`,`check_uuid`
		FROM `resource`
		WHERE id = #{id} AND is_delete = 0
		LIMIT 1
	</select>
	
	<select id="selectByCode" resultType="ResourceDo" parameterType="String">
		select `id`,`name`,`code`,`description`,`check_uuid`
		FROM `resource`
		WHERE `code` = #{code} AND `is_delete` = 0
		LIMIT 1
	</select>
</mapper>